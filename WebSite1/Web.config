		<add name="ApplicationServices" connectionString="data source=.\SQLEXPRESS;Integrated Security=SSPI;AttachDBFilename=|DataDirectory|\aspnetdb.mdf;User Instance=true" providerName="System.Data.SqlClient"/>
		<add name="semester_plannerConnectionString" connectionString="User Id=root;Host=localhost;Database=semester_planner" providerName="Devart.Data.MySql"/>
		<add name="semester_plannerConnectionString2" connectionString="User Id=root;Host=localhost;Database=semester_planner" providerName="Devart.Data.MySql"/>
	</connectionStrings>
	<system.web>
		<compilation debug="true" targetFramework="4.0">
			<assemblies>
				<add assembly="MySql.Data, Version=6.8.7.0, Culture=neutral, PublicKeyToken=C5687FC88969C44D"/>
				<add assembly="System.Design, Version=4.0.0.0, Culture=neutral, PublicKeyToken=B03F5F7F11D50A3A"/>
				<add assembly="System.Web.Extensions.Design, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35"/></assemblies></compilation>
